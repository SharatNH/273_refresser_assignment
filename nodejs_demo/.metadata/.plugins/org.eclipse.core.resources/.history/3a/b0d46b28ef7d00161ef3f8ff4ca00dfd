<script>

var university = { 
  name : "San jose state university",
  address : "one washington sqaure, san jose",
  state : "california ",
  summary : function(){
  return university.name +  "<br>" + university.address +  "<br>" + university.state ;
  }
  };

  document.getElementById("uni").innerHTML = university.summary();
</script>
